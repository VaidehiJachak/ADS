#include <stdio.h> 
#include <stdlib.h> 
#include <string.h> 
#define SIZE 26 struct node {
struct node *child[SIZE]; int end;
};
struct node* newNode() {
struct node *p = (struct node*)malloc(sizeof(struct node)); p->end = 0;
for (int i = 0; i < SIZE; i++) p->child[i] = NULL;
return p;
}
char lower(char c) {
if (c >= 'A' && c <= 'Z') return c + 32;
return c;
}
void insert(struct node *root, char *w) { for (int i = 0; w[i]; i++) {
int id = lower(w[i]) - 'a'; if (!root->child[id])
root->child[id] = newNode(); root = root->child[id];
}
root->end = 1;
}
int search(struct node *root, char *w) { for (int i = 0; w[i]; i++) {
int id = lower(w[i]) - 'a'; if (!root->child[id])
return 0;
root = root->child[id];
}
return root->end;
}
int main() {
char *keys[] = { "and","bat","ball","book","cot","cotton", "internet","interview","joy","job",
"King","Lion","man","mango","manage"
};
int n = 15;
struct node *root = newNode(); for (int i = 0; i < n; i++)
insert(root, keys[i]); printf("Keys:\n");
for (int i = 0; i < n; i++) printf("%s\t", keys[i]);
char word[50];
printf("\n\nEnter word to search: "); scanf("%s", word);
if (search(root, word)) printf("Found");
else
printf("Not Found"); return 0;
}
